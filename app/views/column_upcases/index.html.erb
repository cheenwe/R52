<p id="notice"><%= notice %></p>

<h1>Column Upcases</h1>

<table>
  <thead>
    <tr>
      <th>Contect</th>
      <th>Result</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @column_upcases.each do |column_upcase| %>
      <tr>
        <td><%= column_upcase.contect %></td>
        <td><%= column_upcase.result %></td>
        <td><%= link_to 'Show', column_upcase %></td>
        <td><%= link_to 'Edit', edit_column_upcase_path(column_upcase) %></td>
        <td><%= link_to 'Destroy', column_upcase, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Column Upcase', new_column_upcase_path %>
