<p id="notice"><%= notice %></p>

<h1>系统配置</h1>

<table>
  <thead>
    <tr>
      <th>Var</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sy_configs.each do |sy_config| %>
      <tr>
        <td><%= sy_config.var %></td>
        <td><%= sy_config.value %></td>
        <td><%= link_to 'Show', sy_config %></td>
        <td><%= link_to 'Edit', edit_sy_config_path(sy_config) %></td>
        <td><%= link_to 'Destroy', sy_config, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sy Config', new_sy_config_path %>
